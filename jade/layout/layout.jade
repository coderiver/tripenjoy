doctype html
html
  head
    meta(charset="utf-8")
    block title
      title Template
    //- meta(name="viewport" content="initial-scale=1")
    meta(name="viewport" content="width=1040")
    link(rel="stylesheet" media="all" href="css/screen.css")
    <!--[if IE]><script src="js/html5shiv.js"></script><![endif]-->
  body
